{
  "name": "codetimetracker",
  "displayName": "Code Time Tracker",
  "description": "Track Coding Time in VS Code",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./src/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/BackendExpert/CodeTimeTracker"
  },
  "contributes": {
    "commands": [{
      "command": "extension.showCodingTime",
      "title": "Coding Time"
    }]
  },

  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
